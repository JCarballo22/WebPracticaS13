
@{
    ViewData["Title"] = "Contactos";
}

<h1>Contactos</h1>
<div>
    <section>
        <h2>Formulario</h2>
        <form action="" method="">
            <label for="Nombre">Ingrese su nombre</label>
            <input type="text" value="Escribe tu nombre" />
            <br />
            <label for="Apellido">Ingrese su apellido</label>
            <input type="text" placeholder="Escribe tu apellido" />
            <br />
            <label for="Contraseña">Ingrese una contraseña</label>
            <input type="password" placeholder="Escribre tu contraseña" />
            <br />
            <label>Tipo de datos</label>
            <input type="checkbox" name="" /><span>String</span>
            <input type="checkbox" name="" /><span>Doble</span>
            <input type="checkbox" name="" /><span>Integer</span>
            <input type="checkbox" name="" /><span>Boolean</span>
            <br />
            <label>Genero</label>
            <input type="radio" name="" /><span>Femenino</span>
            <input type="radio" name="" /><span>Masculino</span>
            <br />
            <label>Selecciona un color</label>
            <input type="color" />
            <br />
            <label>Selecciona un rango</label>
            <input type="range" nim="1" max="10" />
            <br />
            <label>Elege una fecha</label>
            <input type="date" name="" />
            <br />
            <label>Ingresa una fecha</label>
            <input type="datetime" name=""/>
            <br />
            <label>Ingresa un numero</label>
            <input type="number" name="" />






        </form>
    </section>
</div>

